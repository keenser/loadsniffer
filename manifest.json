{
    "name": "LoadSniffer",
    "version": "0.0.0",
    "description": "chrome.webRequest sniffer",
    "background": {
	"scripts": [
            "mrc.js",
            "background.js"
        ]
    },
    "permissions": [
        "tabs",
        "contextMenus",
        "webRequest",
        "cookies",
        "*://*/*"
    ],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "128": "icons/grey_128x128.png",
        "48": "icons/grey_48x48.png",
        "32": "icons/grey_32x32.png",
        "16": "icons/grey_16x16.png"
    },
    "manifest_version": 2
}
