{
    "name": "LoadSniffer",
    "version": "0.0.0",
    "description": "chrome.webRequest sniffer",
    "background": {
	"scripts": [
            "background.js"
        ]
    },
    "permissions": [
        "tabs",
	"webRequest",
	"*://*/*"
    ],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["*://*/*"],
            "js": ["content.js"]
        }
    ],
    "manifest_version": 2
}
