{
    "name": "LoadSniffer",
    "version": "0.0.0",
    "description": "chrome.webRequest sniffer",
    "background": {
	"scripts": [
            "background.js"
        ]
    },
    "permissions": [
        "tabs",
        "contextMenus",
        "webRequest",
        "cookies",
        "*://*/*"
    ],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "manifest_version": 2
}
